{
  "name": "satudmb-desktopapp-new",
  "version": "1.0.1",
  "description": "An Electron application with React",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/lab": "^7.0.1-beta.21",
    "@mui/material": "^7.3.7",
    "@mui/styled-engine-sc": "^7.3.7",
    "@mui/x-data-grid": "^8.25.0",
    "@reduxjs/toolkit": "^2.11.2",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/lodash": "^4.17.23",
    "@uidotdev/usehooks": "^2.4.1",
    "apexcharts": "^5.3.6",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "dexie": "^4.3.0",
    "electron-updater": "^6.3.9",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "lodash": "^4.17.23",
    "lucide-react": "^0.562.0",
    "moment": "^2.30.1",
    "node-thermal-printer": "^4.6.0",
    "qrcode.react": "^4.2.0",
    "react-apexcharts": "^1.9.0",
    "react-dropzone": "^14.4.0",
    "react-is": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.12.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.3.6",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "vite": "^7.2.6"
  },
  "build": {
    "productName": "SatuDMB",
    "appId": "com.satudmb.desktop",
    "asar": true,
    "publish": [
      {
        "provider": "github",
        "owner": "DMBMaster",
        "repo": "frontend_dmbapp_desktop",
        "private": true
      }
    ],
    "directories": {
      "buildResources": "resources"
    },
    "extraResources": [
      "./resources/**"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "icon": "resources/ico.ico",
      "target": [
        "nsis"
      ]
    }
  }
}
